@model codeFirst.Models.Marks

@{
    ViewData["Title"] = "Details";
}

<h1>Marks Details</h1>

<div>
    <h4>Marks</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Subject</dt>
        <dd class="col-sm-10">@Model.Subject</dd>

        <dt class="col-sm-2">Score</dt>
        <dd class="col-sm-10">@Model.Score</dd>

        <dt class="col-sm-2">Student</dt>
        <dd class="col-sm-10">@Model.Student?.Name</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.MarksId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
